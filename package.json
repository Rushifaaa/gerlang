{
  "name": "gerlang",
  "version": "0.1.0",
  "description": "German Programming Language",
  "keywords": [
    "programming language"
  ],
  "homepage": "https://github.com/Rushifaaa/gerlang#readme",
  "bugs": {
    "url": "https://github.com/Rushifaaa/gerlang/issues"
  },
  "license": "GPL-3.0-or-later",
  "author": "Rushifaaa <jakub.gencer@protonmail.com>",
  "contributors": [
    "Rushifaaa <jakub.gencer@protonmail.com>",
    "Michael Federczuk <federczuk.michael@protonmail.com>"
  ],
  "main": "out/index.js",
  "bin": {
    "gerlang": "bin/gerlang"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Rushifaaa/gerlang.git"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint $(find . -type f \\( \\( -wholename './src/*' -name '*.ts' \\) -or -wholename './bin/*' \\))",
    "build": "./node_modules/.bin/tsc",
    "clean": "rm -rf ./out ./types",
    "rebuild": "npm run clean && npm run build",
    "test": "npm run lint && npm run build",
    "start": "npm run build && ./bin/gerlang",
    "install": "npm run build",
    "preuninstall": "npm run clean",
    "prepublishOnly": "npm run test"
  },
  "dependencies": {
    "@types/node": "^13.9.1",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "eslint": "^6.8.0"
  }
}
